cks();\n                            Mix_PlayChannel(-1, backSfx, 0);\n                        }\n                        else if (SDL_GetTicks() - rPressedTime >= 500)\n                        {\n                            gamescreen->init();\n                        }\n                        break;\n                \t}\n                }\n                break;\n        }\n    }\n}\n\nvoid Game::clean()\n{\n\twindow.cleanUp();\n\tSDL_Quit();\n}\n\nvoid Game::render()\n{\n\tswitch(gameState)\n\t{\n\tcase TitleState:\n\t\ttitlescreen->render();\n\t\tbreak;\n\tcase IngameState:\n\t\tgamescreen->render();\n\t\tbreak;\n\tcase EndState:\n\t\tendscreen->render();\n\t\tbreak;\n\tcase ManualState:\n\t\tmanualscreen->render();\n\t\tbreak;\n\tcase OptionState:\n\t\toptionscreen->render();\n\t\tbreak;\n\tdefault:\n\t\tbreak;\n\t}\n}\n\nbool Game::running() {\n\treturn isRunning;\n}",
